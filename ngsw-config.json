{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js",
          "/*.(svg|cur|jpg|jpeg|png|webp|gif|otf|ttf|woff|woff2|ani)" 
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "!/assets/icons/**" 
        ]
      }
    },
    {
      "name": "critical-assets",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/**" 
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "catalogos-api",
      "urls": [
        "/api/categorias",
        "/api/conceptos",
        "/api/formas-pago",
        "/api/cuentas",
        "/api/personas",
        "/api/clientes"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1d",
        "timeout": "5s"
      }
    }
  ],
  "navigationUrls": [
    "/**",
    "!/**/*.*",
    "!/**/*__*",
    "!/api/**",
    "!/hangfire/**"
  ]
}